datasource db {
    provider = "sqlite"
    url      = "file:./cdr.db"
}

generator client {
    provider = "prisma-client-js"
    output   = "./generated/sqlite"
}

model PbxCdr {
    id                    Int     @id @default(autoincrement())
    call_id               Int?
    answered              Boolean
    destination_caller_id String?
    ringing_duration      String?
    source_caller_id      String?
    start_time            String?
    talking_duration      String?
    recording_url         String?
    segment_id            Int?     @unique
    cdr_id                String?  @unique
    call_history_id       String?  @default("default_value")
}
